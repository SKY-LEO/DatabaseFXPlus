      <TableView fx:id="brigadesGrid" layoutX="12.0" layoutY="14.0" prefHeight="170.0" prefWidth="526.0" />
      <Button fx:id="btnAddLine" layoutX="466.0" layoutY="391.0" mnemonicParsing="false" onMouseClicked="#btn_AddLine_Click" text="Add line">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnFilterByDistrict" layoutX="62.0" layoutY="233.0" mnemonicParsing="false" onMouseClicked="#btn_FilterByBrigadeProfile_Click" text="Filter by brigade profile">
